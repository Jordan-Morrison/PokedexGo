<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jordanmorrison.pokedexgo" version="2.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="UniversalAnalytics">
        <param name="android-package" value="com.danielcwilson.plugins.analytics.UniversalAnalyticsPlugin" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Pokedex Go</name>
    <description>
        A fully featured Pokedex application for Pokemon Go that allows users to compare Pokemon.
    </description>
    <author email="morr0560@algonquinlive.com" href="https://github.com/Jordan-Morrison">
        Jordan Morrison
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="mdpi" src="res/icon/android/icon_48_mdpi.png" />
    <icon density="hdpi" src="res/icon/android/icon_72_hdpi.png" />
    <icon density="xhdpi" src="res/icon/android/icon_96_xhdpi.png" />
    <icon density="xxhdpi" src="res/icon/android/icon_144_xxhdpi.png" />
    <icon density="xxxhdpi" src="res/icon/android/icon_192_xxxhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
